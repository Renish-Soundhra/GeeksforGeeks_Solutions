class Solution {
    void pushZerosToEnd(int[] arr) {
        int len=arr.length;
        int ctr=0;
        for(int i=0;i<len;i++)
        {
            if(arr[i]!=0)
            {
                int temp=arr[ctr];
                arr[ctr]=arr[i];
                arr[i]=temp;
                ctr++;
            }
        }
    }
}
